<?xml version="1.0"?>
<registry>

  <records prefix="plone.bundles/yafowil"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="enabled">True</value>
    <value key="jscompilation">yafowil.plone.js</value>
    <value key="csscompilation">yafowil.plone.css</value>
    <value key="compile">False</value>
    <value key="depends">plone</value>
  </records>

  <!-- bootstrap glyphicons -->
  <record name="bootstrap.glyphicons">
    <field type="plone.registry.field.Bool">
      <title>Glyphicons Halflings Web Font</title>
    </field>
    <value>True</value>
  </record>

</registry>
